geom_line(aes(y=pred))+
ggtitle(paste("Undir ", limit,sep = ""))+
annotate("text",x=limit-50,y=0.4,label= paste("fjoldi nemenda sem fóru lengra er",hashA%>%
filter(fsfat>limit) %>% summarise(n_distinct(studentId)), sep = " " ))
return(p)
}
